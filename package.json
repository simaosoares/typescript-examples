{
  "name": "typescript-examples",
  "version": "1.0.0",
  "description": "Simple project containing TypeScript examples.",
  "scripts": {
    "clean": "rm -rf ./compiled_sources",
    "clean:all": "npm run clean && npm run test:clean",
    "compile": "tsc",
    "build": "npm run clean && npm run compile",
    "lint": "eslint --ext .ts  ./",
    "run": "node ./compiled_sources/index.js",
    "start": "npm run build && npm run run",
    "test:clean": "rm -rf ./compiled_tests",
    "test:compile": "tsc --outDir ./compiled_tests",
    "test": "npm run test:clean && npm run test:compile && node ./config/jasmine.runner.js"
  },
  "author": "Sim√£o Soares",
  "license": "ISC",
  "keywords": [
    "inversifyjs",
    "basic",
    "example",
    "dependency",
    "injection",
    "javascript",
    "typescript"
  ],
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/node": "^16.6.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "inversify": "^5.1.1",
    "jasmine": "^3.7.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  }
}
